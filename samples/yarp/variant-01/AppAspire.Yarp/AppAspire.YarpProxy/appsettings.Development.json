{
  "Logging": 
  {
    "LogLevel": 
    {
      "Default": "Information"
    }
  },
  "ReverseProxy": 
  {
    "Routes": 
    {
      "blazorweb1" : 
      {
        "ClusterId": "blazorweb1",
        "Match": 
        {
          "Path": "/web1/{**catch-all}",
          "Hosts" : 
          [ 
            "localhost"
          ]
        }
      },
      "blazorweb2" : 
      {
        "ClusterId": "blazorweb2",
        "Match": 
        {
          "Path": "/web2/{**catch-all}",
          "Hosts" : 
          [ 
            "localhost"
          ]
        }
      }
    },
    "Clusters": 
    {
      "blazorweb1": 
      {
        "Destinations": 
        {
          "blazorweb1": 
          {
            "Address": "http://localhost:8100"
          }
        }
      },
      "blazorweb2": 
      {
        "Destinations": 
        {
          "blazorweb2": 
          {
            "Address": "http://localhost:8200"
          }
        }
      }
    }
  }
}
